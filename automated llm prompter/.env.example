# LLM Pipeline Environment Variables
# Copy this file to .env and fill in your actual credentials.
# DO NOT commit the .env file to version control.

# For Gemini API Client
GEMINI_API_KEY="YOUR_GEMINI_API_KEY_HERE"

# For SSH Mixtral Client
MIXTRAL_SSH_HOST="your_ssh_host_ip_or_domain"
MIXTRAL_SSH_USER="your_ssh_username"
MIXTRAL_SSH_KEY_PATH="/path/to/your/private/ssh/key_or_name_if_in_ssh_agent_config" # e.g., ~/.ssh/id_rsa or C:/Users/YourUser/.ssh/id_rsa

# For Local Llama Client (e.g., Ollama)
LLAMA_API_BASE_URL="http://localhost:11434/v1" # Example for Ollama default
# LLAMA_API_KEY="ollama" # Optional: Uncomment and set if your local Llama setup requires an API key